<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.fiap.coleta.entities.Disponibilidade" table="disponibilidade">
	
		<id name="id" column="disponibilidade_id">
			<generator class="increment"/>
		</id>
		
		<many-to-one name="no" not-null="true"/>
		
		<property name="status" not-null="true"/>
		<property name="tempo" not-null="true"/>	
		<property name="data" not-null="true"/>		

	</class> 
</hibernate-mapping>