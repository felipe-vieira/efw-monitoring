<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.fiap.coleta.entities.Indisponibilidade" table="indisponibilidade">
	
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		
		<many-to-one name="no" not-null="true"/>
		
		<property name="inicio" not-null="true"/>	
		<property name="fim"/>		
		

	</class> 
</hibernate-mapping>